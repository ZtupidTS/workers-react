{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "browser-preview",
      "request": "launch",
      "name": "Browser Preview: Launch",
      "url": "http://localhost:8080/",
      "webRoot": "${workspaceRoot}/src",
      "sourceMapPathOverrides": {
        "webpack:///./src/*.js": "${workspaceRoot}/src/*.js"
      }
    }
  ]
}
